# https://docs.oracle.com/en-us/iaas/Content/Block/References/samplefiocommandslinux.htm
[global]
bs=64K
iodepth=64
direct=1
ioengine=libaio
group_reporting
time_based
ramp_time=5
runtime=120
numjobs=4
name=raw-randreadwrite
rw=randrw

[job1]
filename=/dev/sda:/dev/sdb:/dev/sdc:/dev/sdd:/dev/sde:/dev/sdf:/dev/sdg:/dev/sdh:/dev/sdi:/dev/sdj